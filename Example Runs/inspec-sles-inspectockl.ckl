# sed -i "/^[^#]*pam_tally2.so/ c\auth required pam_tally2.so deny=3
onerr=fail even_deny_root unlock_time=86400 root_unlock_time=300"
/etc/pam.d/common-auth-vmware.local</ATTRIBUTE_DATA>
